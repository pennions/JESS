{
  "name": "jess",
  "version": "1.0.0",
  "description": "Stylesheet influenced by Pico CSS and Fluent UI from Microsoft ",
  "main": "index.js",
  "scripts": {
    "jess": "node ./node_modules/less/bin/lessc src/jess.less dist/jess.css",
    "jess-reset": "node ./node_modules/less/bin/lessc src/utilities/reset-css.less dist/jess-reset.css",
    "jess-flex-layout": "node ./node_modules/less/bin/lessc src/layout/jess-flex-layout.less dist/jess-flex-layout.css",
    "build": "npm run jess | npm run jess-flex-layout | npm run jess-reset"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pennions/JESS.git"
  },
  "keywords": [
    "Fluent",
    "CSS",
    "LESS"
  ],
  "author": "Jelmer Veen <www.github.com/pennions>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pennions/JESS/issues"
  },
  "homepage": "https://github.com/pennions/JESS#readme",
  "devDependencies": {
    "less": "^4.1.2"
  }
}
